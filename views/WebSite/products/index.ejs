<%- include("../partial/header.ejs") %>
    <main>
        <!-- product start -->
        <section class="py-5">
            <div class="container-fluid">
                <div class="row ">
                    <% for(var i=0; i < data.length; i++) { %>
                        <div class="col-md-4">
                            <div class="card">
                                <img src="/uploadproductimage/<%= data[i].Images[0] %>" class="card-img-top" width="40%" alt="...">
                                <div class="card-body">
                                    <h5 class="card-title">
                                        <%= data[i].Title %>
                                    </h5>
                                    <p class="card-text">
                                        <%= data[i].Description %>
                                    </p>
                                    <a href="/product/<%= data[i].Pid %>" class="btn bttn">Know more</a>
                                </div>
                            </div>
                        </div>
                        <% } %>
                </div>
            </div>
        </section>
        <!-- product end -->

    </main>
    <%- include("../partial/footer.ejs") %>