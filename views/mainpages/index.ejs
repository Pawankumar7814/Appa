    <main>
        <section>
        </section>
        <section>
            <div class="container">
                <form action="">
                    <center>
                        <i><u><h2> Contact Us</h2></u></i>
                    </center>
                    <br>
                    <div class="row">
                        <div class="row mb-3">
                            <label for="UNAME" class="col-sm-2 col-form-label"> <b>Name</b></label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="UNAME">
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label for="UContact" class="col-sm-2 col-form-label"> <b>Phone No </b></label>
