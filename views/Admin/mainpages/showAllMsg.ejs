<%- include("../partial/header.ejs") %>


    <div class="container-fluid">
        <div class="row border-5">
            <div class="col-12 ">
                <div class=" border row font-weight-bold text-center">
                    <div class=" border col-md-2">
                        Name
                    </div>
                    <div class=" border col-md-3">
                        Email
                    </div>
                    <div class="border col-md-1">
                        Phone
                    </div>
                    <div class="border col-md-2">
                        Msg
                    </div>
                    <div class="border col-md-2">
                        Date
                    </div>
                    <div class="border col-md-1">
                        Resolved
                    </div>
                    <div class="border col-md-1">
                        Action
                    </div>
                </div>
                <br>
            </div>
        </div>
        <div class="row border">
            <div class="col-12 border">
                <% data.forEach(data=> { %>
                    <div class=" border row">
                        <div class=" border col-md-2">
                            <%- data.FName %>
                        </div>
                        <div class=" border col-md-3">
                            <p>
                                <%- data.Email %>
                            </p>
                        </div>
                        <div class="border col-md-1">
                            <%- data.Phone %>
                        </div>
                        <div class="border col-md-2">
                            <%- data.Msg %>
                        </div>
                        <div class="border col-md-2">
                            <%- data.added_date.getDate() %>/
                                <%- data.added_date.getMonth() %>/
                                    <%- data.added_date.getFullYear() %>
                        </div>
                        <div class="border col-md-1">
                            <%- data.resolved %>
                        </div>
                        <div class="border col-md-1">
                            <acronym title="Change Status">
                                <svg width="30px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512">
                                    <a class="btn" href="/Admin/MsgStatus/<%- data.id %>">
                                        <path
                                            d="M182.6 246.6C170.1 259.1 149.9 259.1 137.4 246.6L57.37 166.6C44.88 154.1 44.88 133.9 57.37 121.4C69.87 108.9 90.13 108.9 102.6 121.4L159.1 178.7L297.4 41.37C309.9 28.88 330.1 28.88 342.6 41.37C355.1 53.87 355.1 74.13 342.6 86.63L182.6 246.6zM182.6 470.6C170.1 483.1 149.9 483.1 137.4 470.6L9.372 342.6C-3.124 330.1-3.124 309.9 9.372 297.4C21.87 284.9 42.13 284.9 54.63 297.4L159.1 402.7L393.4 169.4C405.9 156.9 426.1 156.9 438.6 169.4C451.1 181.9 451.1 202.1 438.6 214.6L182.6 470.6z" />
                                    </a>
                                </svg>
                            </acronym>
                            <acronym title="Give FeedBack">
                                <svg width="30px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 512">
                                    <a class="btn" href="/Admin/MsgFeedBack/<%- data.id %>">
                                        <path
                                            d="M416 176C416 78.8 322.9 0 208 0S0 78.8 0 176c0 39.57 15.62 75.96 41.67 105.4c-16.39 32.76-39.23 57.32-39.59 57.68c-2.1 2.205-2.67 5.475-1.441 8.354C1.9 350.3 4.602 352 7.66 352c38.35 0 70.76-11.12 95.74-24.04C134.2 343.1 169.8 352 208 352C322.9 352 416 273.2 416 176zM599.6 443.7C624.8 413.9 640 376.6 640 336C640 238.8 554 160 448 160c-.3145 0-.6191 .041-.9336 .043C447.5 165.3 448 170.6 448 176c0 98.62-79.68 181.2-186.1 202.5C282.7 455.1 357.1 512 448 512c33.69 0 65.32-8.008 92.85-21.98C565.2 502 596.1 512 632.3 512c3.059 0 5.76-1.725 7.02-4.605c1.229-2.879 .6582-6.148-1.441-8.354C637.6 498.7 615.9 475.3 599.6 443.7z" />
                                    </a>
                                </svg>
                            </acronym>
                        </div>
                    </div>
                    <br>
                    <% }) %>
            </div>
        </div>
    </div>



    <%- include("../partial/footer.ejs") %>