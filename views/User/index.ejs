<%- include("../partial/header.ejs") %>
    <main class="bgr">
        <section>
            <div class="container-fluid col">

                <div class="row ">

                    <div class="col-12 col-md-6 ">
                        <div class="cardadjust">
                            <div class="card carddesign">
                                <div class="card-body cardcont">
                                    <h5 class="card-title">Log In</h5>
                                    <h6 class="card-subtitle mb-2 text-muted">New User?<a href="/User/Register">Register
                                            now</a></h6>
                                    <p class="card-text">
                                        <form method="post">
                                            <div>
                                                <label for="emailid">Email ID:</label>
                                                <input type="email" class="form-control col" id="uemail" placeholder="@gmail.com" size="20" required>
                                            </div>
                                            <div>
                                                <label for="pass">Password:</label>
                                                <input type="password" class="form-control col" id="upass" placeholder="At least 6 characters" size="20" required>
                                            </div>
                                            <br>
                                            <br>
                                            <div>
                                                <a href="/User/Signup" class=" btn buttondisp">Sign Up</a>
                                            </div>
                                        </form>

                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>
    </main>
    <%- include("../partial/footer.ejs") %>